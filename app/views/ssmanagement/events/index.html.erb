<div class="jumbotron">
  <a data-toggle="collapse" href="#collapseOne" class="btn btn-default"
    aria-expanded="false" aria-controls="collapseOne" role="button">
    Event form
  </a>
  <p></p>
  <div class="collapse" id="collapseOne" aria-expanded="false">
    <%= simple_form_for Event.new, :url => ssmanagement_events_path do |f| %>
      <%= render partial: "form", locals: {f: f} %>
    <% end %>
  </div>
</div>
<p></p>
<p></p>
<div class="events">
  <%= render @events %>
</div>
